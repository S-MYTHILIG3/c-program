#include <stdio.h>

int main()
{
    int n;
    printf("enter the size:");
    scanf("%d",&n);
    int a[n],i,j,p,s,c=0;
    printf("enter the elements:");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("prime numbers are:");
    for(i=0;i<n;i++)
    {
        c=0;
        for(j=2;j<=a[i]/2;j++)
        {
            if(a[i]%j==0)
            {
                c=1;
                break;
            }
        }
        if(c==0)
        p=a[i];
        
        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<n;j++)
            {
                printf(" ");
            }
            for(int p=i;p<=i;p++)
            {
                printf("%d",p);
            }
            printf("\n");
        }
        
    }

    return 0;

}


