#include <stdio.h>

int
main ()
{
  int i, j, n;
  printf ("enter no.of rows:");
  scanf ("%d", &n);
  for (i = 1; i <= n; i++)
    {
      for (j = 1; j <= n; j++)
	{
	  if (i == 1 || i == n || j == 1 || j == n)
	  {
	      printf("1");
	  }
	      else if(i==4||i==2||j==2||j==4)
	  
	    {
	        
	      printf ("*");
	    }
	  else
	    {
	      printf (" ");

	    }

	}
      printf ("\n");
    }

  return 0;
}
