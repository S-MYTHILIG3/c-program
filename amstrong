#include <stdio.h>

int main()
{
    int a,n,s,i,r;
    printf("enter a number:");
    scanf("%d",&a);
    n=i;
    while(i>0)
    {
        r=a%10;
        s=s+r*r*r;
        n=n/10;
    }
    if(n==s)
    {
        printf("it is a amstrong number");
    }
    else
    {
        printf("it is a amstrong number");
    }
    return 0;
}
