#include <stdio.h>

int main()
{
    int n,s,r,count=0;
    printf("enter a number :");
    scanf("%d",&n);
    while(n>0)
    {
        r=n%2;
        s=s+r;
        n=n/2;
    }
    if(r==1)
    {
       count++;
        if(count%2==0)
        {
            printf("even");
        }
        else
        {
            printf("odd");
        }
    }

    return 0;
}
